{
  "name": "sf-pro-toolkit",
  "version": "1.5.0",
  "description": "SuccessFactors Pro Toolkit - Enterprise Chrome extension for SAP SuccessFactors professionals",
  "private": true,
  "scripts": {
    "build": "node scripts/build.js",
    "build:zip": "node scripts/build.js && cd dist && zip -r ../sf-pro-toolkit-v${npm_package_version}.zip . -x '*.DS_Store'",
    "clean": "rm -rf dist *.zip",
    "version:bump:patch": "npm version patch --no-git-tag-version && node scripts/sync-version.js",
    "version:bump:minor": "npm version minor --no-git-tag-version && node scripts/sync-version.js",
    "version:bump:major": "npm version major --no-git-tag-version && node scripts/sync-version.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sidbhat/sf-pro-toolkit.git"
  },
  "keywords": [
    "sap",
    "successfactors",
    "chrome-extension",
    "productivity",
    "enterprise"
  ],
  "author": "Sid Bhat",
  "license": "MIT",
  "engines": {
    "node": ">=14.0.0"
  },
  "devDependencies": {
    "sharp": "^0.34.5",
    "terser": "^5.44.1"
  }
}
